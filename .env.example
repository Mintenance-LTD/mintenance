# Supabase Configuration
# Get these from your Supabase dashboard: Settings > API
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-public-key-here

# AI Services Configuration
# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your-openai-api-key-here
# AWS Configuration (Alternative to OpenAI)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
# Google Cloud Vision (Alternative to OpenAI)
GOOGLE_CLOUD_API_KEY=your-google-cloud-api-key

# Stripe Configuration - Get from https://dashboard.stripe.com/apikeys
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
# Stripe Webhook Endpoint Secret
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Push Notifications
# Get from https://expo.dev/accounts/[username]/projects/[project]/credentials
EXPO_PUSH_TOKEN=ExponentPushToken[your-push-token-here]

# App Configuration
EXPO_PUBLIC_APP_NAME=Mintenance
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=development

# API Configuration
EXPO_PUBLIC_API_URL=https://your-project-id.supabase.co/rest/v1

# File Upload Configuration
# AWS S3 for image storage
S3_BUCKET_NAME=mintenance-job-photos
S3_ACCESS_KEY_ID=your-s3-access-key
S3_SECRET_ACCESS_KEY=your-s3-secret-key
S3_REGION=us-east-1