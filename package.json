{
  "name": "mintenance-clean",
  "version": "1.1.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:staging": "eas build --platform android --profile staging",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:stable": "eas build --platform android --profile stable",
    "build:android:prod": "eas build --platform android --profile production",
    "build:android:store": "eas build --platform android --profile production-store",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:prod": "eas build --platform ios --profile production",
    "submit:android": "eas submit --platform android --latest",
    "submit:ios": "eas submit --platform ios --latest",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate-credentials": "node scripts/validate-credentials.js",
    "generate-assets": "node scripts/create-minimal-assets.js",
    "beta": "node scripts/beta-testing-tools.js",
    "beta:init": "node scripts/beta-testing-tools.js init",
    "beta:status": "node scripts/beta-testing-tools.js status",
    "beta:export": "node scripts/beta-testing-tools.js export",
    "test:coverage": "jest --coverage",
    "type-check": "tsc -p tsconfig.app.json --noEmit",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android:release": "detox test --configuration android.emu.release",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test": "detox test --configuration android.emu.debug",
    "e2e:android": "npm run e2e:build:android && npm run e2e:test:android",
    "e2e:ios": "npm run e2e:build:ios && npm run e2e:test:ios",
    "e2e:clean": "detox clean-framework-cache && detox build --configuration android.emu.debug",
    "test:mobile": "npm run e2e:android",
    "bundle:analyze": "npx expo export --analyzer",
    "bundle:export": "expo export --platform android --dev false",
    "performance:check": "npm run bundle:export && npm run bundle:analyze",
    "performance:monitor": "node -e \"console.log('Performance monitoring integrated into app. Check logs during development.')\"",
    "performance:report": "node -e \"const fs = require('fs'); if (fs.existsSync('bundle-analysis-report.json')) { console.log(fs.readFileSync('bundle-analysis-report.json', 'utf8')); } else { console.log('No performance report found. Run npm run performance:check first.'); }\"",
    "fix:typescript": "node scripts/fix-typescript-warnings.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:all": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "quality:check": "npm run type-check && npm run lint && npm run format:check && npm test",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "npx audit-ci --moderate",
    "security:licenses": "npx license-checker --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC'",
    "bundle:size": "npx bundlesize",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=json --output-path=./performance-reports/lighthouse.json",
    "optimize:images": "npx imagemin src/assets/images/* --out-dir=src/assets/images/optimized",
    "deploy:staging": "npm run build:web && npm run security:audit && npm run test:coverage",
    "deploy:production": "npm run quality:check && npm run security:check && npm run bundle:analyze && npm run build:web"
  },
  "dependencies": {
    "@expo/cli": "^0.24.21",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@sentry/react-native": "^6.20.0",
    "@stripe/stripe-react-native": "^0.51.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "expo": "~53.0.22",
    "expo-application": "~6.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-haptics": "^14.1.4",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "^7.1.7",
    "expo-local-authentication": "^16.0.5",
    "expo-location": "^18.1.6",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^14.2.3",
    "expo-sqlite": "^15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "i18next": "^25.4.2",
    "react": "19.0.0",
    "react-i18next": "^15.7.2",
    "react-native": "0.79.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-deck-swiper": "^2.0.19",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-localize": "^3.2.1",
    "react-native-maps": "^1.25.3",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.2",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.5.0",
    "sentry-expo": "^7.2.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-preset-expo": "^13.2.4",
    "detox": "^20.40.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.0.0",
    "flipper-js": "^1.0.8",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "metro-symbolicate": "^0.83.1",
    "prettier": "^3.6.2",
    "react-native-performance": "^5.1.4",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
