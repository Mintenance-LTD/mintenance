{
  "name": "@mintenance/mobile",
  "version": "1.2.4",
  "private": true,
  "description": "Mintenance Mobile App (React Native/Expo)",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit": "eas submit",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .expo node_modules",
    "align:expo": "expo install",
    "align:rn": "expo install react-native",
    "align:deps": "expo install react-native react-native-reanimated react-native-screens react-native-safe-area-context",
    "validate:env": "node ../../scripts/validate-credentials.js",
    "analyze": "npx expo export --platform android --output-dir ./dist && du -sh ./dist",
    "analyze:ios": "npx expo export --platform ios --output-dir ./dist && du -sh ./dist",
    "check:performance": "node ../../scripts/check-performance-budgets.js mobile",
    "prebuild": "npm run validate:env",
    "prestart": "npm run validate:env",
    "predev": "npm run validate:env",
    "postinstall": "cd ../.. && npm run build:packages || true"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@mintenance/api-client": "file:../../packages/api-client",
    "@mintenance/design-tokens": "file:../../packages/design-tokens",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.4",
    "@react-native/babel-plugin-codegen": "^0.81.4",
    "@react-navigation/bottom-tabs": "^7.5.0",
    "@react-navigation/drawer": "^7.7.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.3",
    "@sentry/react-native": "7.2.0",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.5",
    "expo": "54.0.23",
    "expo-application": "~7.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "18.0.10",
    "expo-dev-client": "6.0.17",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "8.0.8",
    "expo-local-authentication": "^17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "^0.32.12",
    "expo-splash-screen": "31.0.10",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "3.0.8",
    "expo-system-ui": "6.0.8",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "15.0.9",
    "i18next": "^25.6.2",
    "react": "19.2.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.3.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "4.1.1",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1",
    "sentry-expo": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "19.2.7",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "expo-secure-store": "~15.0.7",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "react-test-renderer": "^19.2.0",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "overrides": {
    "react-native": "0.81.5",
    "@react-native/assets-registry": "0.81.5",
    "@react-native/babel-plugin-codegen": "0.81.5",
    "@react-native/babel-preset": "0.81.5",
    "@react-native/codegen": "0.81.5",
    "@react-native/debugger-frontend": "0.81.5",
    "@react-native/dev-middleware": "0.81.5",
    "@react-native/normalize-colors": "0.81.5"
  }
}
