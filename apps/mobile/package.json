{
  "name": "@mintenance/mobile",
  "version": "1.2.3",
  "private": true,
  "description": "Mintenance Mobile App (React Native/Expo)",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit": "eas submit",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .expo node_modules",
    "align:expo": "expo install",
    "align:rn": "expo install react-native",
    "align:deps": "expo install react-native react-native-reanimated react-native-screens react-native-safe-area-context",
    "validate:env": "node ../../scripts/validate-credentials.js",
    "analyze": "npx expo export --platform android --output-dir ./dist && du -sh ./dist",
    "analyze:ios": "npx expo export --platform ios --output-dir ./dist && du -sh ./dist",
    "check:performance": "node ../../scripts/check-performance-budgets.js mobile",
    "prebuild": "npm run validate:env",
    "prestart": "npm run validate:env",
    "predev": "npm run validate:env"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@mintenance/shared": "file:../../packages/shared",
    "@mintenance/types": "file:../../packages/types",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@stripe/stripe-react-native": "^0.54.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.5",
    "expo": "~54.0.20",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~6.0.16",
    "expo-font": "~14.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "^17.0.7",
    "expo-location": "~18.1.0",
    "expo-notifications": "^0.32.12",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~15.0.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.29.0",
    "react-native-maps": "^1.18.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.21.2",
    "@sentry/react-native": "^5.34.0",
    "sentry-expo": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.2.2",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "expo-secure-store": "~15.0.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "react-test-renderer": "^19.2.0",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
