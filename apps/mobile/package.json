{
  "name": "@mintenance/mobile",
  "version": "1.2.4",
  "private": true,
  "description": "Mintenance Mobile App (React Native/Expo)",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:staging": "eas build --platform android --profile staging",
    "build:ios": "eas build --platform ios",
    "submit": "eas submit",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .expo node_modules",
    "align:expo": "expo install",
    "align:rn": "expo install react-native",
    "align:deps": "expo install react-native react-native-reanimated react-native-screens react-native-safe-area-context",
    "validate:env": "node ../../scripts/validate-credentials.js",
    "setup:eas-env": "node set-eas-env.js",
    "analyze": "npx expo export --platform android --output-dir ./dist && du -sh ./dist",
    "analyze:ios": "npx expo export --platform ios --output-dir ./dist && du -sh ./dist",
    "check:performance": "node ../../scripts/check-performance-budgets.js mobile",
    "prebuild": "npm run validate:env",
    "prestart": "npm run validate:env",
    "predev": "npm run validate:env",
    "postinstall": "cd ../.. && npm run build:packages || true"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@mintenance/api-client": "file:../../packages/api-client",
    "@mintenance/design-tokens": "file:../../packages/design-tokens",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-native/babel-plugin-codegen": "^0.82.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/drawer": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@sentry/react-native": "~7.7.0",
    "@stripe/stripe-react-native": "0.57.0",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-query": "^5.50.0",
    "expo": "~52.0.0",
    "expo-application": "~6.0.0",
    "expo-build-properties": "~0.13.0",
    "expo-constants": "~17.0.0",
    "expo-dev-client": "~5.0.0",
    "expo-device": "~7.0.0",
    "expo-font": "~13.0.0",
    "expo-haptics": "~14.0.0",
    "expo-image-picker": "~16.0.0",
    "expo-linear-gradient": "~14.0.0",
    "expo-linking": "~7.0.0",
    "expo-local-authentication": "~15.0.0",
    "expo-location": "~18.0.0",
    "expo-notifications": "~0.32.13",
    "expo-splash-screen": "~0.29.0",
    "expo-sqlite": "~15.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.0",
    "expo-updates": "~0.26.0",
    "expo-web-browser": "~14.0.0",
    "i18next": "^23.10.0",
    "react": "19.2.0",
    "react-dom": "18.3.1",
    "react-i18next": "^14.1.0",
    "react-native": "0.82.1",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-maps": "1.26.18",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.0.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.6.1",
    "sentry-expo": "~7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^12.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.0",
    "expo-secure-store": "~14.0.0",
    "jest": "^30.2.0",
    "jest-expo": "~52.0.0",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "react-native": "0.76.1"
  }
}